{
  "$schema": "https://json.schemastore.org/azure-functions.json",
  "bindings": [
      {
          "authLevel": "anonymous",
          "type": "httpTrigger",
          "direction": "in",
          "name": "req",
          "methods": [ "GET", "POST", "OPTIONS" ]
      },
      {
          "type": "http",
          "direction": "out",
          "name": "$return"
      }
  ]
}
