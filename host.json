{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "",
      "maxOutstandingRequests": 200,
      "maxConcurrentRequests": 200,
      "dynamicThrottlesEnabled": true
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "http": {
    "cors": {
      "allowedOrigins": [
        "http://127.0.0.1:8081",
        "http://localhost:7071"
      ],
      "supportCredentials": false
    }
  }
}
